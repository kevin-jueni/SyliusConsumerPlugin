{
    "name": "kjueni/sylius-consumer-plugin",
    "type": "sylius-plugin",
    "description": "Integrates Sylius with message processing for sylake/akeneo-producer-bundle.",
    "license": "MIT",
    "authors": [
        {
            "name": "Arkadiusz Krakowiak"
        },
        {
            "name": "Kamil Kokot",
            "email": "kamil@kokot.me",
            "homepage": "http://kamil.kokot.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "php-amqplib/rabbitmq-bundle": "^1.12",
        "sylius-labs/rabbitmq-simplebus-bundle": "^1.0",
        "sylius/sylius": "^1.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^5.6",
        "sylius-labs/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sylake\\SyliusConsumerPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sylake\\SyliusConsumerPlugin\\": "tests/",
            "spec\\Sylake\\SyliusConsumerPlugin\\": "spec/"
        }
    }
}
